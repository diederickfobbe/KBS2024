<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="User_Interface.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:User_Interface"
    Shell.FlyoutBehavior="Flyout"
    Title="SwiftKey">
    
    <FlyoutItem Title="Loginscherm" >
        <ShellContent
        
        ContentTemplate="{DataTemplate local:Loginscherm}"
        Route="ResultPage" />
    </FlyoutItem>
    <FlyoutItem Title="Oefeningscherm">
        <ShellContent
        
        ContentTemplate="{DataTemplate local:Oefenscherm}"
        Route="ResultPage" />
    </FlyoutItem>

</Shell>

